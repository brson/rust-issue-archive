    "url": "https://api.github.com/repos/rust-lang/rust/issues/comments/136681741",
    "html_url": "https://github.com/rust-lang/rust/pull/24270#issuecomment-136681741",
    "issue_url": "https://api.github.com/repos/rust-lang/rust/issues/24270",
