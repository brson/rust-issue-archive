    "issue_url": "https://api.github.com/repos/rust-lang/rust/issues/9909",
    "id": 26656227,
    "node_id": "MDEyOklzc3VlQ29tbWVudDI2NjU2MjI3",
    "user": {
      "login": "thestinger",
      "id": 1505226,
      "node_id": "MDQ6VXNlcjE1MDUyMjY=",
      "avatar_url": "https://avatars.githubusercontent.com/u/1505226?v=4",
