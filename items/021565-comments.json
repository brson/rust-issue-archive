  {
    "url": "https://api.github.com/repos/rust-lang/rust/issues/comments/71358582",
    "html_url": "https://github.com/rust-lang/rust/pull/21565#issuecomment-71358582",
    "issue_url": "https://api.github.com/repos/rust-lang/rust/issues/21565",
    "id": 71358582,
